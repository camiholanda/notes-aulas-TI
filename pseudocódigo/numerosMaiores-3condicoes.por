programa {
  funcao inicio() {

    inteiro num1, num2, num3

    escreva("Informe 3 números diferentes: ")
    leia(num1, num2, num3)

       se(num1 > num2  e num1 > num3)
    {
       escreva("Numero 1 é maior.")
    }
      senao se(num2 > num3)
    {
      escreva("Numero 2 é maior.")
    }
      senao
    {
        escreva("Numero 3 é maior")
    }
    
  
  }
}
